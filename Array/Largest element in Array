class Solution {
    public static int largest(int[] arr) {
        int n=arr.length;
        int max=arr[0];
        
        for (int i=0; i<n; i++){
            if (arr[i] > max){
                max = arr[i];
            }
        }
        return max;
       
    }
    
    public static void main(String[] args){
        int[] arr1={1,8,7,56,66};
        int ans= largest(arr1);
        System.out.println("The Largest element in the array is: " + ans);        
    }
}
